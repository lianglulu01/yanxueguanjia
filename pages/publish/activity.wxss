.showImageBox{
  position: relative;
}
.showImageDel{
  position: absolute;
right: 0rpx;
top: 1rpx;
width: 33rpx;
height: 36rpx;
border-radius: 50%;
border: solid 1px #696969;
color: #696969;
z-index: 10;
}

.showImageDel:before, .showImageDel:after {
  position: absolute;
  left: 16rpx;
  top:4rpx;
  content: ' ';
  height: 30rpx;
  width: 4rpx;
  background-color: #696969;
}
.showImageDel:before {
  transform: rotate(45deg);
}
.showImageDel:after {
  transform: rotate(-45deg);
}

.show-image {
  width: 125rpx;
  height: 125rpx;
  margin:20rpx
}

.xingcheng-image {
  width: 30%;
  height: 22.5vw;
  margin-right: 5%;
}
.xingcheng-image image{
  width: 100%;
  height: 100%;
}
.xingcheng-image .showImageDel{
  right: -16rpx;
  top: -16rpx;
}

.xingcheng-image:nth-child(3n){
  margin: 0;
}

